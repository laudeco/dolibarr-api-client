{
    "name": "laudeco/dolibarr-client",
    "license": "MIT",
    "description": "PHP client for the rest API of Dolibarr.",
    "type": "library",
    "require": {
        "php" : ">=5.6",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer": "~1.1",
        "webmozart/assert": "^1.2",
        "doctrine/collections": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "authors": [
        {
            "name": "De Coninck Laurent",
            "email": "lau.deconinck@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dolibarr\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dolibarr\\Client\\Tests\\": "tests/"
        }
    }

}
